{
    "name": "mediact/static-production-dependency-prototype",
    "description": "A prototype package to test static production dependency analysis tooling.",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "MediaCT",
            "email": "info@mediact.nl"
        },
        {
            "name": "Jan-Marten de Boer",
            "role": "developer"
        },
        {
            "name": "Ashoka de Wit",
            "role": "developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "symfony/symfony": "^4.0",
        "nikic/php-parser": "^3.0",
        "composer/composer": "^1.6",
        "zero-config/cli": "^1.4"
    },
    "require-dev": {
        "mediact/testing-suite": "@stable"
    },
    "autoload": {
        "classmap": [
            "src/symfony"
        ],
        "exclude-from-classmap": [
            "**ParentNotExists**",
            "**Prototype/BadClasses**",
            "**Doctrine/Test**",
            "**Fixture**",
            "**DbalLogger**",
            "**LazyLoadingValue**",
            "**AdapterTest**"
        ],
        "psr-4": {
            "Mediact\\DependencyGuard\\": "src"
        }
    },
    "extra": {
        "grumphp": {
            "config-default-path": "vendor/mediact/testing-suite/config/default/grumphp.yml"
        },
        "dependency-guard": {
            "exclude": [
                "Symfony\\Component\\DependencyInjection\\Tests\\Fixtures\\Prototype\\BadClasses\\",
                "Symfony\\Component\\VarDumper\\Tests\\Fixtures\\NotLoadableClass",
                "Symfony\\Component\\Config\\Tests\\Fixtures\\Bad*",
                "Symfony\\Component\\Cache\\Tests\\"
            ],
            "ignore": [
                "monolog/monolog",
                "phpunit/*"
            ]
        }
    },
    "archive": {
        "exclude": [
            "/.gitignore",
            "/phpunit.xml",
            "/phpmd.xml",
            "/phpstan.neon",
            "/phpcs.xml",
            "/bitbucket-pipelines.yml"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
